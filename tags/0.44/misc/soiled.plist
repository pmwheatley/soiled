<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Enabled</key>
	<true/>
	<key>Label</key>
	<string>Flash-Policy</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/cat</string>
		<string>/ABSOLUTE/PATH/TO/flashpolicy.xml</string>
	</array>
	<key>inetdCompatibility</key>
	<dict>
		<key>Wait</key>
		<false/>
	</dict>
	<key>Sockets</key>
	<dict>
		<key>Listeners</key>
		<dict>
			<key>SockServiceName</key>
			<string>843</string>
		</dict>
	</dict>
</dict>
</plist>
